{
  "type": "module",
  "name": "otterhttp-session",
  "description": "Simple promise-based session utility for otterhttp",
  "keywords": [
    "otterhttp",
    "session",
    "promise"
  ],
  "main": "./lib/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./lib/index.js"
    }
  },
  "types": "lib/session.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "lint": "eslint src --ext ts --ignore-path .gitignore",
    "prepack": "pnpm build",
    "test:import": "cd test/import-test && rm -rf node_modules && yarn && node index.js && node index.cjs && cd ../../",
    "test:unit": "jest --coverageReporters=text-lcov > coverage.lcov",
    "test": "yarn test:unit && yarn test:import",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OtterJS/otterhttp-session.git"
  },
  "author": "lordfirespeed <hello@lordfirespeed.dev>",
  "license": "LGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/OtterJS/otterhttp-session/issues"
  },
  "homepage": "https://github.com/OtterJS/otterhttp-session#readme",
  "devDependencies": {
    "@types/cookie": "^0.4.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "jest": "^27.2.0",
    "light-my-request": "^4.4.4",
    "ts-jest": "^27.0.5",
    "tsup": "^8.2.4",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "cookie": "^0.4.1",
    "nanoid": "^3.1.25"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@9.8.0+sha256.56a9e76b51796ca7f73b85e44cf83712862091f4d498c0ce4d5b7ecdc6ba18f7"
}
